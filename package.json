{
  "name": "nuxt-koa-basic",
  "version": "1.0.0",
  "description": "My delightful Nuxt.js project",
  "author": "Whidy",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "cross-env NODE_ENV=development nodemon -w server -w nuxt.config.js server/index.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:pm2": "pm2 start npm --name=nuxt-koa-basic -- start",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cookie-universal-nuxt": "^2.0.16",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.14",
    "element-ui": "^2.10.1",
    "koa": "^2.7.0",
    "minireset.css": "0.0.5",
    "nuxt": "^2.8.1",
    "vue-clipboard2": "^0.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "1.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-jest": ">=22.7.2",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "precss": "^4.0.0"
  }
}
